@Test
public void SwapTest1()
{
 String[] array = {"1", "2", "3","4"};
 app.Swap(1,3);
 assertTrue(4, array[1]);
}

@Test
public void SwapTest2()
{
 String[] array = {"1", "2", "3","4"};
 app.Swap(2,3);
 assertTrue(4, array[2]);
}

@Test
public void SwapTest3()
{
 String[] array = {"1", "2", "3","4"};
 app.Swap(3,1);
 assertTrue(2, array[3]);
}

@Test
public void SwapTest4()
{
 String[] array = {"1", "2", "3","4"};
 app.Swap(0,2);
 assertTrue(3, array[0]);
}
